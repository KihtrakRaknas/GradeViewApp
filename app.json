      "supportsTablet": true,
      "infoPlist": {
        "NSNotificationDescription": "This app uses notifications to notify you of changes in your grades",
        "NSFaceIDUsageDescription": "Face ID is used when you enable biometric security"
      },
      "bundleIdentifier": "com.kihtrak.REGEN",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8985838748167691~8580642842"
      }
    },
    "android": {
      "package": "com.kihtrak.REGEN",
      "versionCode": 23,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8985838748167691~8887764609"
      }
    },
    "userInterfaceStyle": "automatic",
    "description": "App client for the GradeView"
  }
}
