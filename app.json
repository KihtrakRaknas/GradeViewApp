{
  "expo": {
    "name": "GradeView",
    "slug": "Regen",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "4.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "facebookAppId": "618501142264378",
    "facebookDisplayName": "GradeView",
    "splash": {
      "image": "./assets/newSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#373a6d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
