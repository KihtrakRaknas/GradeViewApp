{
  "expo": {
    "name": "REGEN",
    "slug": "Regen",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.6",
    "orientation": "portrait",
    "icon": "./assets/genIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSNotificationDescription": "This app uses notifications to notify you of changes in your grades"
      },
      "bundleIdentifier": "com.kihtrak.REGEN"
    },
    "android": {
      "package": "com.kihtrak.REGEN"
    },
    "description": "App client for the gradebook that South Brunswick High School uses"
  }
}